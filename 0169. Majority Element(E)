class Solution:
    def majorityElement(self, nums: List[int]) -> int:
        nums.sort()
        max=0
        tmp=0
        res=nums[0]
        for i in range(1,len(nums)):
            
            if nums[i] == nums[i-1]:
                tmp+=1
            else:
                tmp=0
                
            if tmp>max:
                max=tmp
                res = nums[i]
        
        return res
